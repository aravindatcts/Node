{
  "_meta": {
    "sources": [
      "loopback/common/models",
      "loopback/server/models",
      "../common/models",
      "./models"
    ],
    "mixins": [
      "loopback/common/mixins",
      "loopback/server/mixins",
      "../common/mixins",
      "./mixins"
    ]
  },
  "slaMetrics": {
    "dataSource": "mongods",
    "public": true,
    "options": {
      "strictObjectIDCoercion": true
    }
  },
  "slas": {
    "dataSource": "mongods",
    "public": false
  },
  "slacomments": {
    "dataSource": "mongods",
    "public": false
  },
  "pendreasons": {
    "dataSource": "mongods",
    "public": false
  },
  "pendreason": {
    "dataSource": "mongods",
    "public": false
  },
  "User": {
    "dataSource": "mongods",
    "options": {
      "realmRequired": false,
      "realmDelimiter": ":"
    }
  },
  "AccessToken": {
    "dataSource": "mongods",
    "public": false
  },
  "ACL": {
    "dataSource": "mongods",
    "public": false
  },
  "RoleMapping": {
    "dataSource": "mongods",
    "public": false,
    "options": {
      "strictObjectIDCoercion": true
    }
  },
  "Role": {
    "dataSource": "mongods",
    "public": false
  },
  "Settings": {
    "dataSource": "mongods",
    "public": true
  }
}
